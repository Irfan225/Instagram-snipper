#--- Instagram Credentials ---
IG_USERNAME=username
IG_PASSWORD=Password

#--- WhatsApp Bot API ---
WA_BOT_ENDPOINT=http:localhost:3000/wa-api/send

#--- Snipper Settings ---
# Target akun Instagram, pisahkan dengan koma tanpa spasi
TARGET_ACCOUNTS=target

# Interval pengecekan dalam milidetik (misal: 10000 = 10 detik)
POLL_INTERVAL_MS=10000

#--- File Paths (biarkan default jika tidak ada kebutuhan khusus) ---
SESSION_FILE=./ig-session.json
DUP_PERSIST_PATH=./seen.json

#--- Feed Snipper Settings ---
FILTER_KEYWORDS=Online,Registration,Only,Link
SEEN_FEEDS_FILE=./seen-feeds.json